{
  "manifest_version": 2,
  "name": "DNS reviews",
  "version": "1.0.0",
  "browser_action": {
  "default_icon": "icon.png",
  "permissions": [
       "http://127.0.0.1:5000/*",
        "tabs"
    ]
},
  "content_scripts": [
   {
     "matches": [
       "https://www.dns-shop.ru/catalog/*/*/*"
     ],
     "js": ["jquery-3.5.1.slim.min.js", "content.js"]
   }
 ]
}
